{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Accept-Encoding",
					"value": "br"
				},
				{
					"key": "Cache-Control",
					"value": "max-age=0, s-maxage=86400"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				},
				{
					"key": "Referrer-Policy",
					"value": "strict-origin"
				},
				{
					"key": "Permissions-Policy",
					"value": "geolocation=(), microphone=()"
				}
			]
		}
	]
}
